{
    "name": "hbsolutions/swoole-slim-app",
    "description": "Basis for REST API application on Slim 4 and Swoole Server.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "HBS Development Team",
            "email": "dev.hbsolutions@gmail.com"
        }
    ],
    "minimum-stability": "dev",
